{
  "name": "@bg-dev/nuxt-auth",
  "version": "1.4.37-edge",
  "license": "MIT",
  "type": "module",
  "author": "Becem Gharbi",
  "keywords": [
    "nuxt",
    "auth",
    "oauth",
    "edge"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "edge"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prisma:init": "cd playground && prisma init",
    "prisma:generate": "cd playground && prisma generate",
    "prisma:migrate": "cd playground && prisma migrate dev",
    "prisma:push": "cd playground && prisma db push",
    "release": "npm run prepack && changelogen --release && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "@prisma/client": "^5.2.0",
    "bcryptjs": "^2.4.3",
    "defu": "^6.1.2",
    "jose": "^4.14.4",
    "mustache": "^4.2.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.6.5",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/mustache": "^4.2.2",
    "@types/uuid": "^9.0.2",
    "changelogen": "^0.5.5",
    "eslint": "^8.47.0",
    "nuxt": "^3.6.5",
    "prisma": "^5.2.0"
  },
  "peerDependencies": {
    "prisma": "^5.2.0"
  },
  "repository": "https://github.com/becem-gharbi/nuxt-auth.git"
}