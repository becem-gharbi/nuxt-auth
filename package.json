{
  "name": "@bg-dev/nuxt-auth",
  "version": "0.1.5",
  "license": "MIT",
  "type": "module",
  "author": "Becem Gharbi",
  "keywords": [
    "nuxt",
    "authentication",
    "oauth"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prisma:init": "cd playground && prisma init",
    "prisma:generate": "cd playground && prisma generate",
    "prisma:migrate": "cd playground && prisma migrate dev",
    "release": "npm run prepack && changelogen --release && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "@nuxt/kit": "^3.4.1",
    "@prisma/client": "^4.12.0",
    "bcryptjs": "^2.4.3",
    "defu": "^6.1.2",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.1",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/schema": "^3.4.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mustache": "^4.2.2",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^9.0.1",
    "changelogen": "^0.5.3",
    "eslint": "^8.38.0",
    "nuxt": "^3.4.1",
    "prisma": "^4.12.0"
  },
  "peerDependencies": {
    "prisma": "^4.9.0"
  },
  "repository": "https://github.com/becem-gharbi/nuxt-auth.git"
}
