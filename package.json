{
  "name": "@bg-dev/nuxt-auth",
  "version": "2.1.1-rc",
  "license": "MIT",
  "type": "module",
  "author": "Becem Gharbi",
  "keywords": [
    "nuxt",
    "auth",
    "oauth",
    "edge"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prisma:init": "cd playground && prisma init",
    "prisma:generate": "cd playground && prisma generate",
    "prisma:migrate": "cd playground && prisma migrate dev",
    "prisma:push": "cd playground && prisma db push",
    "release": "nuxi typecheck && npm run lint && npm run prepack && changelogen --release --versionSuffix rc && npm publish && git push --follow-tags",
    "lint": "eslint ."
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.1",
    "@prisma/client": "^5.2.0",
    "bcryptjs": "^2.4.3",
    "defu": "^6.1.2",
    "jose": "^4.14.6",
    "mustache": "^4.2.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/bcryptjs": "^2.4.3",
    "@types/mustache": "^4.2.2",
    "@types/uuid": "^9.0.3",
    "changelogen": "^0.5.5",
    "eslint": "^8.48.0",
    "nuxt": "^3.7.1",
    "prisma": "^5.2.0"
  },
  "peerDependencies": {
    "prisma": "^5.2.0"
  },
  "repository": "https://github.com/becem-gharbi/nuxt-auth.git"
}